<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">

   <!-- kreiraj tabelu depratment-->
    <changeSet id="create-department" author="p.vojinovic">
        <sqlFile path="ddl/create-department.sql" relativeToChangelogFile="true"/>
    </changeSet>
   <!-- kreiraj tabelu subject-->
    <changeSet id="create-subject" author="p.vojinovic">
        <sqlFile path="ddl/create-subject.sql" relativeToChangelogFile="true"/>
    </changeSet>

    
    <!-- popuni tabelu depratment-->
    <changeSet id="init-department" author="p.vojinovic">
        <sqlFile path="dml/init-department.sql" relativeToChangelogFile="true"/>
    </changeSet>

    <changeSet id="init-subject" author="p.vojinovic">
        <sqlFile path="dml/init-subject.sql" relativeToChangelogFile="true"/>
    </changeSet>






</databaseChangeLog>
